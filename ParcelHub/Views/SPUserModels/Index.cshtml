@model IEnumerable<ParcelHub.Models.SPUserModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_SPLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SPWarehouseModel.CompanyName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsValid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Role)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SPWarehouseModel.CompanyName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsValid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Role)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ApplicationUserId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ApplicationUserId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ApplicationUserId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
