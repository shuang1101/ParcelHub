@model IEnumerable<ParcelHub.Models.Consumer>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_SPLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ModelIsvalid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MobileNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApplicationUserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateRegisterd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateTimeLastLogin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WechatId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MemeberShipId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ModelIsvalid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MobileNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationUserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateRegisterd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateTimeLastLogin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WechatId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MemeberShipId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
